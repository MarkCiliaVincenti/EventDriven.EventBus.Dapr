{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DaprEventBusOptions": {
    "PubSubName": "pubsub"
  },
  "DaprEventBusSchemaOptions": {
    "UseSchemaRegistry": true,
    "SchemaValidatorType": "SchemaValidatorType.Json",
    "SchemaRegistryType": "SchemaRegistryType.Mongo",
    "AddSchemaOnPublish": true,
    "DaprStateStoreOptions": {
      "StateStoreName": "mongodb-statestore"
    },
    "MongoStateStoreOptions": {
      "ConnectionString": "mongodb://localhost:27017",
      "DatabaseName": "schema-registry",
      "CustomersCollectionName": "schemas"
    }
  }
}
